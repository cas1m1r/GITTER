        missing-searx-pyenv:
           There is no pyenv in ${SEARX_PYENV}
        installer-modified:
           There are files modified locally in the installer (clone),
           see ${SEARX_SRC_INIT_FILES} description.
        python-installed:
           Scripts can be executed in instance's environment
           - user:  ${SERVICE_USER}
           - pyenv: ${SEARX_PYENV}
    
