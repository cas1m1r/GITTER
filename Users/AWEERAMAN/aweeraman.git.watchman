        +       2653 void* std::__1::__thread_proxy<std::__1::tuple<make_new_client(std::__1::unique_ptr<watchman_stream, std::__1::default_delete<watchman_stream> >&&)::$_0> >(void*)  (in watchman) + 437  [0x1093bcde5]
        +         2653 dispatch_command(watchman_client*, json_ref const&, int)  (in watchman) + 333  [0x109396c5d]
        +           2653 cmd_query(watchman_client*, json_ref const&)  (in watchman) + 319  [0x1093956ff]
