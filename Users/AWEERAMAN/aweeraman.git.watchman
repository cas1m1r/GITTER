        +                 2653 default_generators(w_query*, std::__1::shared_ptr<watchman_root> const&, w_query_ctx*, long long*)  (in watchman) + 92  [0x1093aa02c]
        +                   2653 watchman::InMemoryView::timeGenerator(w_query*, w_query_ctx*, long long*) const  (in watchman) + 230  [0x1093757e6]
        +                     2653 w_query_process_file(w_query*, w_query_ctx*, std::__1::unique_ptr<FileResult, std::__1::default_delete<FileResult> >)  (in watchman) + 124  [0x1093a8afc]
        +                       2653 ListExpr::evaluate(w_query_ctx*, FileResult const*)  (in watchman) + 63  [0x1093a573f]
        +                         2653 NotExpr::evaluate(w_query_ctx*, FileResult const*)  (in watchman) + 14  [0x1093a4f3e]
        +                           2653 ListExpr::evaluate(w_query_ctx*, FileResult const*)  (in watchman) + 63  [0x1093a573f]
        +                             2653 SinceExpr::evaluate(w_query_ctx*, FileResult const*)  (in watchman) + 49  [0x1093af371]
        +                               2653 w_clockspec_eval(std::__1::shared_ptr<watchman_root> const&, w_clockspec const*, w_query_since*)  (in watchman) + 65  [0x109380801]
